    text-align: center;
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-around;
}

.icons {
    margin: 0.5em;
}

#icon1, #icon2, #icon3, #icon4 {
    margin: 0.5em;
    font-size: 1.3em;

}


.gallery {
    height: 25em;
}

</script>
